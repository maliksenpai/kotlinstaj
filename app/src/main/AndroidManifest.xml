<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="com.basket.myapplication">

	<uses-permission android:name="android.permission.INTERNET"/>

	<application
			android:allowBackup="true"
			android:icon="@mipmap/ic_launcher"
			android:label="@string/app_name"
			android:roundIcon="@mipmap/ic_launcher_round"
			android:supportsRtl="true"
			android:theme="@style/AppTheme">
		<activity android:name="com.basket.myapplication.Room.RoomActivity">
		</activity>
		<activity android:name=".Firebase">
		</activity>
		<activity android:name=".Fragment.FragmentActivity">
		</activity>
		<activity android:name=".Retrofit.Retrofit">
		</activity>
		<activity android:name=".Bars">
		</activity>
		<activity android:name=".Glide">
		</activity>
		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
	</application>

</manifest>